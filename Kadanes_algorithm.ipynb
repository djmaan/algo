   "source": [
    "# Kadanes Algorithm\n",
    "# o(n) time | o(1) space\n",
    "def kadanesAlgorithm(array):\n",
    "    maxEndingAlgorithm = array[0]\n",
    "    maxSoFar = array[0]\n",
    "    for num in array[1:]:\n",
    "        maxEndingHere = max(num, maxEndingHere + num)\n",
    "        maxSoFar = max(maxSoFar, maxEndingAlgorithm)\n",
    "    return maxSoFar"
   ]
  }
